          "program": "${workspaceFolder}/extern/godot-engine/bin/godot.linuxbsd.editor.dev.x86_64",
          "miDebuggerPath": "/usr/bin/gdb"
        },
        "windows": {
          "program": "C:/Users/Administrator/Downloads/Godot_v4.2.1-stable_win64.exe/Godot_v4.2.1-stable_win64.exe",
          "miDebuggerPath": "gdb"
        },
        "args": [
          "--debug",
          "--verbose",
          "--path",
          "${workspaceFolder}/demo"
        ],
        "cwd": "${workspaceFolder}",
        "visualizerFile": "${workspaceFolder}/.vscode/godot.natvis",
        "internalConsoleOptions": "openOnSessionStart",
